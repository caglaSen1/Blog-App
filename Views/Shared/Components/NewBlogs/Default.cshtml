@model List<Blog>

<div class="card mt-4">
    <div class="card-header">
        <h5 class="card-title">Recently Added Blogs</h5>
    </div>
    <div class="card-body">
        @foreach (var blog in Model)
        {
            <div class="mb-3">
                <h6 class="mb-0">
                    @blog.Title

                </h6>
            </div>
        }
    </div>
</div>
