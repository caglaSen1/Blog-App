@model List<Blog>

<div class="card mt-4">
    <div class="card-header">
        <h5 class="card-title">Recently Added Blogs</h5>
    </div>
    <div class="card-body">
        @foreach (var blog in Model)
        {
            <div class="mb-3">
                <h6 class="mb-0">
                    <a href="/blog/details/@blog.Url">
                        @blog.Title
                    </a>
                </h6>
            </div>
        }
    </div>
</div>
